<%= form_with(url: search_users_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "名前を検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>
<h3>
ユーザーを全て表示する
</h3>
<ul class='item-lists'>
   <li>
   苗字
   名前
   住所
   市町村区
   担当
   </li>
  <% @users.each do |user| %>
   <li>
    <%= user.last_name %>
    <%= user.first_name %>
    <%= user.address %>
    <%= user.city %>
    <%= user.address%>
    <%= user.staff[:name] %>
   </li>
  <% end %>
</ul>